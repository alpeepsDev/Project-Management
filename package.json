{
  "name": "capstone",
  "version": "1.0.0",
  "description": "TaskForge - Shape ideas into results",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "start": "concurrently \"npm run server:start\" \"npm run client:start\"",
    "server:dev": "cd server && npm run dev",
    "server:start": "cd server && npm start",
    "client:dev": "cd client && npm run dev",
    "client:start": "cd client && npm run build && npm run preview",
    "prisma:generate": "cd server && npm run prisma:generate",
    "prisma:push": "cd server && npm run prisma:push",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json,css,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alpeepsDev/Project-Management.git"
  },
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/alpeepsDev/Project-Management/issues"
  },
  "homepage": "https://github.com/alpeepsDev/Project-Management#readme",
  "dependencies": {
    "concurrently": "^9.2.1"
  },
  "devDependencies": {
    "prettier": "^3.6.2"
  }
}
